<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Name</th>
      <th>Race</th>
      <th>Class</th>
      <th>Campaign</th>
   </tr>
  </thead>
  <tbody>
    <% characters.forEach(function(c) { %>
      <tr>
        <td><%= c.name %></td>
        <td><%= c.race %></td>
        <td><%= c.class %></td>
        <td></td>
        <td><a href="/characters/<%= c._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
  
<%- include('../partials/footer') %>